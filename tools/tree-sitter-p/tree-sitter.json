{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "p",
      "camelcase": "P",
      "title": "P",
      "scope": "source.p",
      "file-types": [
        "p"
      ],
      "injection-regex": "^p$",
      "class-name": "TreeSitterP",
      "highlights": "queries/highlights.scm",
      "indents": "queries/indents.scm",
      "folds": "queries/folds.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for the P programming language.",
    "authors": [
      {
        "name": "Pablo Hugen",
        "email": "phugen@redhat.com",
        "url": "https://tomcat0x42.me/"
      }
    ],
    "links": {
      "repository": "https://github.com/Tomcat-42/p"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}